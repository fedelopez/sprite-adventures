<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.bowerbreak.sprites.app">

    <application
            android:allowBackup="true"
            android:icon="@drawable/ic_launcher"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
        <activity
                android:name="com.bowerbreak.sprites.app.MainActivity"
                android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.bowerbreak.sprites.app.About" android:label="@string/about_title_label"/>
        <activity android:name="com.bowerbreak.sprites.app.GameActivity"/>
        <activity android:name="com.bowerbreak.sprites.app.SpriteActivity"/>
    </application>

</manifest>
